{
  "name": "wp/multisite",
  "type": "project",
  "description": "Hale multisite required packages",
  "license": "MIT",
  "authors": [],
  "keywords": [
    "composer",
    "multisite",
    "php",
    "stack",
    "wordpress",
    "wp"
  ],
  "support": {
    "issues": "https://github.com/ministryofjustice/hale-platform/issues"
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "config": {
    "allow-plugins": true
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://connect.advancedcustomfields.com"
    },
    {
      "type": "composer",
      "url": "https://packagist.org"
    },
    {
      "type": "composer",
      "url": "https://wp-languages.github.io"
    },
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    },
    {
      "type": "vcs",
      "url": "https://github.com/ministryofjustice/cookie-compliance.git"
    },
    {
      "type": "vcs",
      "url": "https://github.com/ministryofjustice/govwind.git"
    },
    {
      "type": "vcs",
      "url": "https://github.com/ministryofjustice/hale-components.git"
    },
    {
      "type": "vcs",
      "url": "https://github.com/ministryofjustice/hale-dash.git"
    },
    {
      "type": "vcs",
      "url": "https://github.com/ministryofjustice/hale-showcase.git"
    },
    {
      "type": "vcs",
      "url": "https://github.com/ministryofjustice/hale.git"
    },
    {
      "type": "vcs",
      "url": "https://github.com/ministryofjustice/ppo.git"
    },
    {
      "type": "vcs",
      "url": "https://github.com/ministryofjustice/website-builder-blocks.git"
    },
    {
      "type": "vcs",
      "url": "https://github.com/ministryofjustice/wp-gov-uk-notify.git"
    },
    {
      "type": "vcs",
      "url": "https://github.com/ministryofjustice/wp-moj-blocks.git"
    },
    {
      "type": "vcs",
      "url": "https://github.com/ministryofjustice/wp-s3-uploads.git"
    },
    {
      "type": "package",
      "package": {
        "name": "relevanssi/relevanssi-premium",
        "version": "2.29.0",
        "type": "wordpress-plugin",
        "dist": {
          "type": "zip",
          "url": "https://relevanssi.com/update/get_version.php?version=2.29.0"
        }
      }
    }
  ],
  "require": {
    "composer/installers": "^v2.0",
    "ministryofjustice/cookie-compliance": "1.0.3",
    "ministryofjustice/govwind": "dev-main",
    "ministryofjustice/hale": "4.30.0",
    "ministryofjustice/hale-components": "1.13.0",
    "ministryofjustice/hale-dash": "1.1.8",
    "ministryofjustice/hale-showcase": "1.1.10",
    "ministryofjustice/ppo": "dev-main",
    "ministryofjustice/website-builder-blocks": "dev-main",
    "ministryofjustice/wp-gov-uk-notify": "1.0.3",
    "ministryofjustice/wp-moj-blocks": "3.19.5",
    "ministryofjustice/wp-s3-uploads": "2.0.0",
    "oscarotero/env": "^1.0",
    "php": ">=8.0",
    "relevanssi/relevanssi-premium": "2.29.0",
    "sentry/sdk": "^3.3",
    "vlucas/phpdotenv": "^3.0",
    "wpackagist-plugin/auth0": "^4.0.0",
    "wpackagist-plugin/classic-editor": "^1.0",
    "wpackagist-plugin/ewww-image-optimizer": "^8.0",
    "wpackagist-plugin/microsoft-clarity": "^0.10.8",
    "wpackagist-plugin/option-tree": "^2.0",
    "wpackagist-plugin/query-monitor": "^3.0",
    "wpackagist-plugin/remove-category-url": "^1.0",
    "wpackagist-plugin/safe-svg": "^2.0",
    "wpackagist-plugin/simple-301-redirects": "^2.0",
    "wpackagist-plugin/unconfirmed": "^1.0",
    "wpackagist-plugin/wordpress-seo": "^25.0",
    "wpackagist-plugin/wp-force-login": "^5.0",
    "wpackagist-plugin/wp-sentry-integration": "^8.0",
    "wpackagist-plugin/wps-hide-login": "^1.0",
    "wpengine/advanced-custom-fields-pro": "^6.0"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.0"
  },
  "extra": {
    "installer-paths": {
      "wordpress/wp-content/mu-plugins/{$name}/": [
        "type:wordpress-muplugin",
        "ministryofjustice/hale-components",
        "ministryofjustice/wp-gov-uk-notify",
        "ministryofjustice/wp-s3-uploads"
      ],
      "wordpress/wp-content/plugins/{$name}/": [
        "type:wordpress-plugin"
      ],
      "wordpress/wp-content/themes/hale/": [
        "ministryofjustice/hale"
      ],
      "wordpress/wp-content/themes/{$name}/": [
        "type:wordpress-theme"
      ]
    },
    "wordpress-install-dir": "/var/www/html"
  },
  "scripts": {
    "post-root-package-install": [],
    "test": [
      "phpcs"
    ]
  }
}
